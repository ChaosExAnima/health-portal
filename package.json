{
  "name": "health-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' DEBUG='app:*' next dev",
    "predev": "yarn start-db && yarn gen",
    "build": "next build",
    "start": "next start",
    "gen": "graphql-let",
    "orm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "start-db": "docker-compose up -d",
    "stop-db": "docker-compose down",
    "load-schema": "yarn orm schema:sync",
    "load-fixtures": "ts-node lib/db/populate.ts",
    "caniuse": "yarn browserslist --update-db",
    "test": "jest",
    "test:ci": "yarn jest --ci --coverage",
    "test:watch": "yarn jest --watch --watchman",
    "lint": "eslint ."
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@graphql-tools/load-files": "^6.2.5",
    "@graphql-tools/merge": "^6.2.6",
    "@graphql-tools/schema": "^7.1.2",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "apollo-datasource": "^0.7.2",
    "apollo-server-micro": "^2.19.0",
    "apollo-upload-client": "^14.1.3",
    "clsx": "^1.1.1",
    "csv-parser": "^3.0.0",
    "dataloader": "^2.0.0",
    "dayjs": "^1.9.7",
    "debug": "^4.3.1",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "graphql-upload": "^11.0.0",
    "mysql": "^2.18.1",
    "next": "10.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-dropzone": "^11.2.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.30"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/plugin-helpers": "^1.18.2",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.1",
    "@graphql-tools/mock": "^7.0.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.11",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@wordpress/eslint-plugin": "^7.3.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "casual": "^1.6.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "graphql-let": "^0.16.3",
    "jest": "^26.6.3",
    "jest-expect-message": "^1.0.2",
    "sqlite3": "^5.0.2",
    "ts-jest": "^26.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@wordpress/eslint-plugin/recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-console": "warn",
      "prettier/prettier": "warn",
      "jest/no-disabled-tests": "warn"
    },
    "ignorePatterns": [
      "**/*.graphql.d.ts",
      "**/*.graphqls.d.ts",
      ".graphql/**"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "modulePaths": [
      "."
    ]
  }
}
