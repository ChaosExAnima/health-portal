{
  "name": "health-portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "predev": "yarn start-db && yarn gen",
    "build": "next build",
    "start": "next start",
    "gen": "graphql-let",
    "start-db": "docker-compose up -d",
    "stop-db": "docker-compose down",
    "load-schema": "yarn orm schema:sync",
    "load-fixtures": "ts-node lib/db/populate.ts",
    "reset-db": "yarn orm schema:drop && yarn orm schema:sync",
    "caniuse": "yarn browserslist --update-db",
    "test": "jest",
    "test:ci": "yarn jest --ci --coverage",
    "test:watch": "yarn jest --watch --watchman",
    "lint": "eslint ."
  },
  "dependencies": {
    "@material-ui/core": "^4.11",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "clsx": "^1.1.1",
    "csv-parser": "^3.0.0",
    "dayjs": "^1.9.7",
    "debug": "^4.3.1",
    "dotenv": "^10.0.0",
    "knex": "^0.95.6",
    "mysql": "^2.18.1",
    "next": "^11",
    "react": "^17",
    "react-dom": "^17",
    "react-dropzone": "^11.2.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.21",
    "@types/node": "^14",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "@wordpress/eslint-plugin": "^9",
    "babel-jest": "^27",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "casual": "^1.6.2",
    "eslint": "^7.15.0",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-mui-unused-classes": "^1.0.3",
    "jest": "^27",
    "jest-expect-message": "^1.0.2",
    "sqlite3": "^5.0.2",
    "ts-jest": "^27",
    "ts-node": "^10",
    "typescript": "^4.1.2"
  },
  "resolutions": {
    "fs-capacitor": "^6.2.0"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@wordpress/eslint-plugin/esnext",
      "plugin:@wordpress/eslint-plugin/jsdoc",
      "plugin:@wordpress/eslint-plugin/react",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "prettier/react",
      "plugin:@typescript-eslint/recommended"
    ],
    "plugins": [
      "@typescript-eslint",
      "jest"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-console": "warn",
      "prettier/prettier": "warn",
      "jest/no-disabled-tests": "warn",
      "@wordpress/no-unused-vars-before-return": "warn",
      "@wordpress/no-global-event-listener": "off"
    },
    "ignorePatterns": [
      "**/*.d.ts"
    ]
  },
  "prettier": "@wordpress/prettier-config",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "jest-expect-message"
    ],
    "modulePaths": [
      "."
    ]
  }
}
